{
  "name": "codedor/laravel-livewire-forms",
  "description": "Easy to build/use livewire forms.",
  "keywords": [
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "pragmarx/countries": "^0.7.0",
    "livewire/livewire": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Codedor\\LivewireForms\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Codedor\\LivewireForms\\LivewireFormsServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^6.23",
    "pestphp/pest": "^1.21"
  }
}
