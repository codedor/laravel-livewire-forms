{
  "name": "codedor/laravel-livewire-forms",
  "description": "Easy to build/use livewire forms.",
  "keywords": [
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4 || ^8.0",
    "pragmarx/countries": "^0.8",
    "livewire/livewire": "^1.1 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "Codedor\\LivewireForms\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Codedor\\LivewireForms\\LivewireFormsServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-feature/laravel-9": "2.3.0-dev"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
