{
    "name": "codedor/laravel-livewire-forms",
    "description": "Easy to build/use livewire forms.",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "livewire/livewire": "^3.0",
        "petercoles/multilingual-country-list": "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-livewire": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Codedor\\LivewireForms\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedor\\LivewireForms\\LivewireFormsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
