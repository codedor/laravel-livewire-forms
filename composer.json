{
    "name": "codedor/laravel-livewire-forms",
    "description": "Easy to build/use livewire forms.",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "pragmarx/countries": "^0.7.0",
        "livewire/livewire": "^1.0 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Codedor\\LivewireForms\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedor\\LivewireForms\\LivewireFormsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
